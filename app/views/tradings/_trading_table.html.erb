<table>
  <tr>
    <th>Market</th>
    <th><%= type %> Order</th>
    <th>Price Variation</th>
    <th>Market1 Amount</th>
  </tr>
  <% tradings.each do |trade| %>
    <tr>
      <td><%= trade.market.name %></td>
      <td><%= trade.order_count %></td>
      <td><%= trade.price_variation %></td>
      <td><%= trade.market1_amount %></td>
      <td><%= trade.error_message %></td>
    </tr>
  <% end %>
</table>